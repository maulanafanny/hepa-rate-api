{
  "version": 2,
  "builds": [
    {
      "src": "src/api/main.ts",
      "use": "@vercel/node"
    }
  ],
  "paths": {
    "@api/*": ["src/api/*"],
    "@core/*": ["src/core/*"],
    "@test/*": ["./test/*"]
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/api/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
